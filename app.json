{
  "expo": {
    "name": "PDA.OK",
    "slug": "pda-ok",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.michaelmistree.pdataskapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0f0f23"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.michaelmistree.pdataskapp"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7ba133ff-6a63-48eb-9c04-aa10ca1ff89a"
      },
      "EXPO_PUBLIC_TEST_MODE": "true",
      "EXPO_PUBLIC_ANALYSIS_URL": ""
    }
  }
}
